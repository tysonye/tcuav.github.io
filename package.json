{
  "name": "uav-website",
  "private": true,
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "update-sitemap": "node update-sitemap.js",
    "build": "npm run update-sitemap && vite build && node copy-static-files.js",
    "preview": "vite preview",
    "predeploy": "npm run build",
    "deploy": "./node_modules/.bin/gh-pages.cmd -d dist --dotfiles --branch gh-pages --message \"Deploy React app to GitHub Pages\"",
    "test-deploy": "npm run build && npm run preview"
  },
  "dependencies": {
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "swiper": "^12.0.3"
  },
  "devDependencies": {
    "@types/react": "^18.3.27",
    "@types/react-dom": "^18.3.7",
    "@vitejs/plugin-react": "^4.7.0",
    "autoprefixer": "^10.4.23",
    "gh-pages": "^6.3.0",
    "postcss": "^8.5.6",
    "tailwindcss": "^3.4.19",
    "typescript": "^5.7.3",
    "vite": "^6.4.1",
    "vite-plugin-sitemap": "^0.8.2",
    "vite-tsconfig-paths": "^4.3.2"
  },
  "engines": {
    "node": ">=16.0.0",
    "npm": ">=8.0.0"
  },
  "packageManager": "npm@8.0.0"
}
